/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=selectA);
    And(a=selectA, b=a, out=outA); // A is 1 and selected
    And(a=sel, b=b, out=outB);     // B is 1 and selected
    Or(a=outA, b=outB, out=out);   // Otherwise return 0
}